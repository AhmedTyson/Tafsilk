{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.EntityFrameworkCore": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=TafsilkPlatformDb;Trusted_Connection=True;MultipleActiveResultSets=false;TrustServerCertificate=True"
  },
  "Database": {
    "EnableSensitiveDataLogging": true
  },
  "DetailedErrors": true,
  "Features": {
    "EnableSwagger": true,
    "EnableDetailedErrors": true,
    "EnableGoogleOAuth": false
  }
}
